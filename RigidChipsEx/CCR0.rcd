function main()
	kph = math.floor(_VEL(0) * 3.6 + 0.5)

	if _KEY(14) ~= 0 and kph >= 60 then
		ENGINE = 0
		BRAKE = 128
	end

	n0 = math.mod(kph, 10)

	kph = math.floor(kph / 10)

	n1 = math.mod(kph, 10)

	kph = math.floor(kph / 10)

	n2 = math.mod(kph, 10)

	if _KEY(4) == 1 then
		BR=BR+2097152
	end

	if _KEY(5) == 0 then
		if HANDLE > -164 then
			HANDLE = -164
		end
		
		if HANDLE < -196 then
			HANDLE = -196
		end
	end

	_SETCOLOR(0)

	_BOX2D(0.80, -0.625, 0.35, 0.15)
	
	_SETCOLOR(16777215)

	draw_num7[n0](0.7, -0.5, 0.1)

	draw_num7[n1](0.6, -0.5, 0.1)


	draw_num7[n2](0.5, -0.5, 0.1)
end
