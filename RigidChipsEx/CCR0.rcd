Lua
{function _VEL(n)
	return _LEN3( _VX(n), _VY(n), _VZ(n))
end

function _LEN3(a,b,c)
	return math.sqrt(a * a + b * b + c * c)
end

function _BOX2D(x, y, sx, sy)
	for y = y, y + sy, 0.002 do
		_MOVE2D(x, y)
		_LINE2D(x - sx, y)
	end
end

function OnInit()
	_ZOOM(27)
end

